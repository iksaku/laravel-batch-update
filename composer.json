{
    "name": "iksaku/laravel-mass-update",
    "description": "Update multiple Laravel Model records, each with different values, using a single query!",
    "keywords": [
        "iksaku",
        "laravel",
        "laravel-mass-update"
    ],
    "homepage": "https://github.com/iksaku/laravel-mass-update",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Gonz√°lez",
            "email": "yo@jorgeglz.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.13|^11.0",
        "illuminate/support": "^10.13|^11.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0",
        "laravel/pint": "^1.17",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^1.2",
        "phpstan/phpstan-phpunit": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Iksaku\\Laravel\\MassUpdate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Iksaku\\Laravel\\MassUpdate\\Tests\\": "tests",
            "Iksaku\\Laravel\\MassUpdate\\Tests\\App\\": "tests/app",
            "Iksaku\\Laravel\\MassUpdate\\Tests\\Database\\": "tests/database",
            "Iksaku\\Laravel\\MassUpdate\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
